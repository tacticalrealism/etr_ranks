{
    "files.associations": {
        "*.hpp": "ext"
    },
    "sqf.enableCBA": true,
    "sqf.enableACE3": true,
    "sqflint.ignoredVariables": [
        "_input0",
        "_thisArgs",
        "_thisId",
        "_thisType",
        "cba_disposable_replaceDisposableLauncher",
        "cba_disposable_NormalLaunchers",
    ],
    "sqflint.exclude": [],
    "sqflint.includePrefixes": {},
    "psi-header.changes-tracking": {
        "include": ["sqf"],
        "autoHeader": "manualSave"
    },
    "psi-header.lang-config": [
        {
            "license": "Custom",
            "language": "sqf",
            "rootDirFileName": "mod.cpp",
            "beforeHeader": ["#include \"script_component.hpp\""],
            "begin": "/*",
            "prefix": " *",
            "end": " */",
            "blankLinesAfter": 1,
            "forceToTop": true
        }
    ],
    "psi-header.templates": [
        {
            "language": "sqf",
            "template": [
                " Author: <author>",
                " Function description",
                "",
                " Arguments:",
                " 0: Objects <ARRAY>",
                " 1: All <BOOL>",
                "",
                " Return Value:",
                " None",
                "",
                " Example:",
                " [[bob, ted], false] call etr_operation_main_fnc_example",
                "",
                " Public: No"
            ]
        }
    ]
}